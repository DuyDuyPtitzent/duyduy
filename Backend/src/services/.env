# Server Configuration
NODE_ENV=development
PORT=5000

# Database Configuration (MySQL)
DB_HOST=localhost
DB_PORT=3306
DB_USER=your_mysql_user         # << THAY BẰNG USER MYSQL CỦA BẠN
DB_PASSWORD=your_mysql_password # << THAY BẰNG PASSWORD MYSQL CỦA BẠN
DB_NAME=resident_management_db  # << THAY BẰNG TÊN DATABASE CỦA BẠN
DB_DIALECT=mysql

# JWT Configuration
JWT_SECRET=YOUR_VERY_STRONG_AND_UNIQUE_JWT_SECRET_KEY_HERE_CHANGE_IT_NOW  # << THAY BẰNG CHUỖI BÍ MẬT MẠNH
JWT_EXPIRES_IN=1d